{
  "diagnostic.messageTarget": "float",
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSave": true,
  "diagnostic.errorSign": "✗",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "ⓘ",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "‣ ",
  "eslint.run": "onSave",
  "eslint.packageManager": "yarn",
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "eslint.codeAction.disableRuleComment": {
    "enable": true,
    "location": "separateLine"
  },
  "eslint.workingDirectories": ["./ui", "./functions", "."],
  "codeLens.enable": false,
  "suggest.enablePreselect": true,
  "suggest.noselect": false,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "~/dev/dotfiles/.vim/ab_snips"
  ],
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "vim-devicons",
  "explorer.filename.colored.enable": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelintplus.autoFixOnSave": true,
  "stylelintplus.cssInJs": true,
  "snippets.ultisnips.pythonPrompt": false
}
