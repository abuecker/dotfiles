{
    "default_module": "powerline.segments.common",
    "segment_data": {
        "hostname": {
            "before": " ",
            "after": "⋮ ",
            "args": {
                "only_if_ssh": true
            }
        },
        "virtualenv": {
            "before": "℗ "
        },
        "branch": {
            "before": " "
        }
    },
    "segments": {
        "left": [
            {
                "name": "virtualenv"
            },
            {
                "name": "hostname"
            },
            {
                "name": "environment",
                "before": "☁ ",
                "args": {
                    "variable": "CLOUD_NAME"
                }
            },
            {
                "name": "environment",
                "before": "$ ",
                "args": {
                    "variable": "CLOUD_ACCOUNT_NAME"
                }
            },
            {
                "name": "environment",
                "before": "⚑ ",
                "args": {
                    "variable": "CLOUD_REGION"
                }
            },
            {
                "name": "cwd",
                "args": {
                    "dir_limit_depth": 3
                }
            }
        ],
        "right": [
            {
                "module": "powerline.segments.shell",
                "name": "last_pipe_status"
            },
            {
                "name": "branch"
            }
        ]
    }
}
